{
  "name": "sketchmin",
  "version": "1.0.2",
  "description": "Reduces sketch file size by compressing images",
  "main": "index.js",
  "bin": {
    "sketchmin": "index.js"
  },
  "engines": {
    "node": ">=6"
  },
  "directories": {
    "test": "fixtures"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/rimraf ./tmp ./test",
    "lint": "./node_modules/.bin/eslint *.js",
    "test": "npm run lint && npm run testonly",
    "testonly": "npm run clean && ./node_modules/.bin/ava test.js",
    "precommit": "lint-staged",
    "preversion": "npm run clean && npm test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "sketch",
    "sketchmin",
    "compress"
  ],
  "author": "Jake Larson <jake.j.larson@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "archiver": "^2.0.3",
    "fs-extra": "^4.0.1",
    "get-stdin": "^5.0.1",
    "glob": "^7.1.2",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^5.2.2",
    "meow": "^3.7.0",
    "ora": "^1.3.0",
    "rimraf": "^2.6.1",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint": "^4.6.0",
    "execa": "^0.8.0",
    "husky": "^0.14.3",
    "prettier": "^1.6.1",
    "lint-staged": "^4.0.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
